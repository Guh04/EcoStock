	private void Guna2GradientButton2_Click(object sender, EventArgs e)
	{
		Guna2TextBox1.Clear();
		Guna2TextBox2.Clear();
		Guna2TextBox3.Clear();
		Guna2TextBox4.Clear();
	}

	private void Guna2CircleButton1_Click(object sender, EventArgs e)
	{
		MyProject.Forms.Frm_Menu.AbrirFormularioNoPainel<Frm_Cadastro>(MyProject.Forms.Frm_Menu.Panel_Conteudo);
	}

	private void Guna2GradientButton1_Click(object sender, EventArgs e)
	{
		string nome = Guna2TextBox1.Text;
		if (!double.TryParse(Guna2TextBox3.Text, out var preco))
		{
			Debugger.Launch();
			throw new FormatException("Por favor, insira um valor numérico válido para o preço.");
		}
		if (!int.TryParse(Guna2TextBox2.Text, out var quantidade))
		{
			Debugger.Launch();
			throw new FormatException("Por favor, insira um valor inteiro válido para a quantidade.");
		}
		string categoria = Guna2TextBox4.Text;
		Produto produto = new Produto
		{
			nome = nome,
			preco = Convert.ToDouble(decimal.Parse(Conversions.ToString(preco))),
			quantidade = quantidade,
			categoria = categoria
		};
		object[] array;
		bool[] array2;
		NewLateBinding.LateCall(produtoService, null, "CadastrarProduto", array = new object[1] { produto }, null, null, array2 = new bool[1] { true }, IgnoreReturn: true);
		if (array2[0])
		{
			produto = (Produto)Conversions.ChangeType(RuntimeHelpers.GetObjectValue(array[0]), typeof(Produto));
		}
		Guna2TextBox1.Text = "";
		Guna2TextBox3.Text = "";
		Guna2TextBox2.Text = "";
		Guna2TextBox4.Text = "";
		MessageBox.Show("Produto cadastrado com sucesso!", "Sucesso", MessageBoxButtons.OK, MessageBoxIcon.Asterisk);
	}
}
