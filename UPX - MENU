Public class Frm_Menu

Private Sub btn_cadastro_click(sender As Object, e As EventArgs) Handles MyBase.Click
        Frm_cadastro.Show()
        Me.Hide()
    End Sub

Private Sub btn_relatorio_click(sender As Object, e As EventArgs) Handles MyBase.Click
        Frm_relatorio.Show()
        Me.Hide()
    End Sub

Private Sub btn_logout_click(sender As Object, e As EventArgs) Handles MyBase.Click
        Me.close()
        Frm_Login.Show()
    End Sub

Private Sub btn_verde_click(sender As Object, e As EventArgs) Handles MyBase.Click
        Me.hide()
        Frm_verde.Show()
    End Sub





End Class
